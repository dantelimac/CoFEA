{{- '{:>10}'.format(0) }}
{{- '{:>10}'.format(0) -}}{%- if loop.index % 2 == 0 -%}{{"\n"}}{%- endif -%}
{% endfor %}
{{ '{:>6}'.format(-1) }}
{%- endif -%}
{% endfor -%}
{# -------------------------- #}
